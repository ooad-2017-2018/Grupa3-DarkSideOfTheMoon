@{
    ViewBag.Title = "Home Page";
}


<style>
	/* Always set the map height explicitly to define the size of the div
	   * element that contains the map. */
	#map {
		height: 400px;
		width: 700px;
	}
	
	
</style>
<br />



<div class="row">
	<div id="map"></div>
	<script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
			center: { lat: 43.856259, lng: 18.413076},
          zoom: 8
        });
      }
	</script>
	<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDjUiA_l8kOJo4EaAEOo42aT-sYwSq72uE&callback=initMap"
			async defer></script>
</div>