<Page
    x:Class="Dark_Web.View.FormaDetektiv"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Dark_Web.View"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Image Source="../Assets/Pozadina4.jpg" Stretch="Fill" Margin="0,0,-5,0"/>
        <ListView x:Name="lista" Background="White" BorderBrush="Black" BorderThickness="2" HorizontalAlignment="Left" Height="264" VerticalAlignment="Top" Width="472" Margin="112,100,0,0"/>
        <Button  Foreground = "#FF09F909"  Background="#FF615F60" Content="Odjavi se" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="116,32,0,0" Click="Button_Click"/>
        <Button  Foreground = "#FF09F909"  Background="#FF615F60" Content="Podnesi zahtjev" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="630,200,0,0" Click="Button_Click_2"/>
        <TextBox x:Name="izvjestaj" HorizontalAlignment="Left" Margin="114,392,0,0" Text="Napisi izvjestaj o pracenju" VerticalAlignment="Top" Width="462" Height="190" GotFocus="kliknuto"/>
        <Button  Foreground = "#FF09F909"  Background="#FF615F60" Content="Napisi izvjestaj" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="630,520,0,0" Click="Button_Click_1"/>

    </Grid>
</Page>
